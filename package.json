{
  "name": "amplify-jotai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8000",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build": "next build && next export -o dist",
    "lint": "run-s lint:next lint:stylelint lint:prettier",
    "lint:fix": "run-s lint:next:fix lint:stylelint:fix lint:prettier:fix",
    "lint:next": "next lint",
    "lint:next:fix": "next lint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:stylelint": "stylelint ./**/*.{css,scss}",
    "lint:stylelint:fix": "stylelint --fix ./**/*.{css,scss}",
    "release:first": "standard-version -f -t ''",
    "release:major": "standard-version -t '' -r major && git push -u origin release && git push --follow-tags",
    "release:minor": "standard-version -t '' -r minor && git push -u origin release && git push --follow-tags",
    "release:patch": "standard-version -t '' -r patch && git push -u origin release && git push --follow-tags",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^3.0.1",
    "@casl/ability": "^5.4.4",
    "@casl/react": "^3.0.0",
    "@headlessui/react": "^1.6.2",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.8.10",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "aws-amplify": "^4.3.26",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "dayjs": "^1.11.3",
    "env-cmd": "^10.1.0",
    "jotai": "^1.7.2",
    "lodash-es": "^4.17.21",
    "react-daum-postcode": "^3.0.3",
    "react-dropdown-tree-select": "^2.7.1",
    "react-hook-form": "^7.31.1",
    "react-intersection-observer": "^9.1.0",
    "react-query": "^3.39.0",
    "react-toastify": "^9.0.1",
    "standard-version": "^9.3.2",
    "tw-elements": "^1.0.0-alpha12"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "autoprefixer": "10.4.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.1.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "next-export-optimize-images": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "regenerator-runtime": "^0.13.9",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "stylelint-scss": "^4.2.0",
    "typescript": "4.6.4"
  }
}
